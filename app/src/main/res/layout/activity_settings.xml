<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingsVM" type="com.curevivedoctor.app.modules.settings.data.viewmodel.SettingsVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnOne"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_1pxv"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">


        <ScrollView
            android:id="@+id/scrollview"
            android:layout_weight="1"
            android:fillViewport="true"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        <LinearLayout android:gravity="start"
            android:id="@+id/linearSettings"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStacktime"
                android:layout_height="@dimen/_222pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumntime"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_15">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowsearch"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_width="@dimen/_31pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_white_a700"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_search_white_a700"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettingsTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.74"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_settings"
                            style="@style/txtAssistantbold24_3"
                            tools:text="@string/lbl_settings"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDrRayanShaik"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_dr_rayan_shaik"
                        style="@style/txtAssistantbold24_3"
                        tools:text="@string/lbl_dr_rayan_shaik"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNaturopathyDoc"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_75pxh"
                        android:layout_marginStart="@dimen/_75pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/msg_naturopathy_doc"
                        style="@style/txtAssistantsemibold12_2"
                        tools:text="@string/msg_naturopathy_doc"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageImageOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_108pxh"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_110pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_image1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image1"/>
            </FrameLayout>



            <FrameLayout
                android:id="@+id/frameStackprofile"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnprofile"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowarrowright"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_14">
                        <LinearLayout android:id="@+id/linearRowprofile"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_weight="0.94"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <FrameLayout android:id="@+id/frameIconhelp"
                                android:layout_height="@dimen/_33pxv"
                                android:layout_width="@dimen/_32pxh" style="@style/groupStylelight_blue_A700cornerRadius">
                                <ImageView
                                    android:id="@+id/imageProfile"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_17pxh"
                                    android:layout_marginBottom="@dimen/_8pxv"
                                    android:layout_marginEnd="@dimen/_7pxh"
                                    android:layout_marginStart="@dimen/_8pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="@dimen/_17pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_profile"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_profile"/>
                            </FrameLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRaiseaquery"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_weight="0.78"
                                android:layout_width="0dp"
                                android:letterSpacing="0.0"
                                android:singleLine="true"
                                android:text="@string/lbl_raise_a_query"
                                style="@style/txtPoppinsmedium16_5"
                                tools:text="@string/lbl_raise_a_query"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_21pxv"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginStart="@dimen/_146pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="@dimen/_10pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowarrowrightOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_14">
                        <LinearLayout
                            android:id="@+id/linearRowthumbsup"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_weight="0.92"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <FrameLayout
                                android:id="@+id/frameIconfeedback"
                                android:layout_height="@dimen/_31pxv"
                                android:layout_width="@dimen/_31pxh" style="@style/groupStylelight_blue_A700cornerRadius">
                                <ImageView
                                    android:id="@+id/imageThumbsup"
                                    android:layout_gravity="end"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_marginBottom="@dimen/_8pxv"
                                    android:layout_marginEnd="@dimen/_7pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_thumbsup_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_thumbsup_white_a700"/>
                            </FrameLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFeedback"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_weight="0.72"
                                android:layout_width="0dp"
                                android:letterSpacing="0.0"
                                android:singleLine="true"
                                android:text="@string/lbl_feedback"
                                style="@style/txtPoppinsmedium16_5"
                                tools:text="@string/lbl_feedback"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageArrowrightOne"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_22pxv"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginStart="@dimen/_177pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="@dimen/_10pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:gravity="center_horizontal"
                        android:id="@+id/recyclerSettings"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_11pxh}" tools:itemCount="2" tools:listitem="@layout/row_settings"/>
                    <View android:background="@color/gray_403"
                        android:id="@+id/lineLineTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_359pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMore"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_more"
                        style="@style/txtRubikregular18"
                        tools:text="@string/lbl_more"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAboutus"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_about_us"
                        style="@style/txtRubikregular18_1"
                        tools:text="@string/lbl_about_us"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrivacypolicy"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_privacy_policy"
                        style="@style/txtRubikregular18_1"
                        tools:text="@string/lbl_privacy_policy"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtTermAndCondition"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/msg_terms_and_condi"
                        style="@style/txtRubikregular18_1"
                        tools:text="@string/msg_terms_and_condi"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRateusonapp"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{settingsVM.settingsModel.txtRateusonapp}"
                        style="@style/txtRubikregular18_1" tools:text="@string/msg_rate_us_on_app"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumn"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_marginBottom="20dp"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        style="@style/groupStylecornerRadius_3">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnlogout"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            style="@style/groupStylecornerRadius_15">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtLogout"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_136pxh"
                                android:layout_marginStart="@dimen/_136pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{settingsVM.settingsModel.txtLogout}"
                                style="@style/txtPoppinsmedium16_2" tools:text="@string/lbl_logout"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>



        </LinearLayout>

        </ScrollView>
    </LinearLayout>
</layout>
