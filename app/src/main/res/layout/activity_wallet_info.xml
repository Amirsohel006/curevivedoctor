<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modules.WalletFragment">


    <LinearLayout android:id="@+id/linearHeader"
        android:layout_gravity="top|start"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        style="@style/groupStylebluegray_9011">
        <LinearLayout android:id="@+id/linearRowsettings"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_59pxv"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:orientation="horizontal">
            <ImageView android:id="@+id/imageSettings"
                android:layout_height="@dimen/_30pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/white_wallte"
                tools:ignore="ContentDescription"
                tools:src="@drawable/white_wallte"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWallet"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_wallet"
                style="@style/txtPoppinsregular24"
                tools:text="@string/lbl_wallet"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout android:id="@+id/linearColumntotalincome"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginTop="70dp"
        android:orientation="vertical"
        style="@style/groupStyleblue_802cornerRadius">

        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnanalysis"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylecornerRadius_28">
            <LinearLayout android:id="@+id/linearRowanalysis"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/backimage"
                    android:layout_gravity="center"
                    android:src="@drawable/image_white_arrow_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAnalysis"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="6dp"
                    android:text="Total Income"
                    style="@style/txtPoppinsbold18"
                    tools:text="Total Income"/>
                <TextView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_146pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:text="Monthly"
                    style="@style/txtInterregular12"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription"/>
            </LinearLayout>

            <LinearLayout
               android:layout_gravity="start"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    style="@style/txtPoppinsregular24"
                    android:text="@string/lbl_4_599"
                    android:layout_marginStart="16dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="@string/lbl_4_599"/>
                <TextView
                    style="@style/txtPoppinsmedium12_4"
                    android:layout_marginTop="5dp"
                    android:text="Balance Available"
                    android:layout_marginStart="16dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>


            <View android:background="@color/white_A700"
                android:id="@+id/lineLine242One"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="@dimen/_327pxh"/>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:id="@+id/fortrnsaction"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_height="wrap_content">

                <ImageView
                    android:src="@drawable/trnsaction"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


                <ImageView
                    android:src="@drawable/earning"
                    android:layout_marginStart="70dp"
                    android:layout_width="wrap_content"
                    android:layout_height="74dp"/>

                <ImageView
                    android:layout_marginStart="70dp"
                    android:src="@drawable/fines"
                    android:layout_width="wrap_content"
                    android:layout_height="74dp"/>

            </LinearLayout>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackaccounts"
                android:layout_height="@dimen/_537pxv"
                android:layout_marginTop="@dimen/_84pxv"
                android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnline241"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_16">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowaccounts"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnaccounts"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAccounts"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="Transaction"
                                style="@style/txtPoppinsregular23"
                                tools:text="Transaction"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHDFCBank"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="March 31,2022"
                                style="@style/txtPoppinsregular18"
                                tools:text="March 31,2022"/>
                        </LinearLayout>

                    </LinearLayout>




                    <TextView android:ellipsize="end" android:id="@+id/txtFines"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_fines"
                        style="@style/txtPoppinsmedium201"
                        tools:text="@string/lbl_fines"/>
                    <TextView android:ellipsize="end" android:id="@+id/txtDate"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_march_31_2022"
                        style="@style/txtPoppinsregular16"
                        tools:text="@string/lbl_march_31_2022"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowdanielclark"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDanielclark"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_daniel_clark"
                            style="@style/txtPoppinsregular16_11"
                            tools:text="@string/lbl_daniel_clark"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFivethousand"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_175pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_5000"
                            style="@style/txtPoppinsmedium181"
                            tools:text="@string/lbl_5000"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowlawren"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLawren"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_lawren"
                            style="@style/txtPoppinsregular16_11"
                            tools:text="@string/lbl_lawren"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwothousand"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_214pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_2000"
                            style="@style/txtPoppinsmedium181"
                            tools:text="@string/lbl_2000"/>
                    </LinearLayout>

                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_212pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_212pxv"
                    android:layout_width="@dimen/_1pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_black_900"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>




</FrameLayout>