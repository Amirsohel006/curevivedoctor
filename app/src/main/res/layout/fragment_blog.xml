<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".modules.blogfragment.Blog">


    <androidx.appcompat.widget.Toolbar
        android:gravity="center_horizontal"
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        app:contentInsetStart="0dp">
        <LinearLayout
            android:id="@+id/linearRowdrrayanshaik"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal">
            <ImageView android:id="@+id/imageImageOne"
                android:layout_height="@dimen/_35pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="centerCrop"
                android:layout_gravity="center"
                android:src="@drawable/community"
                android:layout_marginStart="20dp"
                tools:ignore="ContentDescription"
                tools:src="@drawable/community"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumndrrayanshaik"
                android:layout_height="wrap_content"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.64"
                android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDrRayanShaik"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="Community"
                    style="@style/txtAssistantbold24"
                    tools:text="Community"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsetwentythree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>


    <ScrollView
        android:clipToPadding="false"
        android:fillViewport="true"
        android:gravity="start"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_width="match_parent">
        <LinearLayout
            android:gravity="start"
            android:id="@+id/linearStudioBookong"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical">
            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tabLayoutGroup2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                app:tabBackground="@drawable/tab_selector_for_blog"
                app:tabGravity="center"
                app:tabIndicatorColor="@android:color/transparent"
                app:tabIndicatorHeight="0dp"
                app:tabMode="fixed"
                app:tabPaddingStart="10dp"
                app:tabSelectedTextColor="@color/black_900"
                app:tabTextColor="@color/black_900"
                android:textSize="10sp"
                style="@style/groupStylewhite_10"
                tools:ignore="SmallSp" />

            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/viewPagerViewpager"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_width="match_parent"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>