<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".modules.homecontainer.HomeContainerActivity">



    <FrameLayout
        android:id="@+id/fragmentContainer"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_width="match_parent"/>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/frameBottombar"
        style="@style/groupStylegray_52cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:paddingTop="10dp"
        app:itemIconTint="@drawable/icon_tinit_selector"
        app:itemTextColor="@drawable/icon_tinit_selector"
        app:menu="@menu/nav_menu_supervisor"
        app:labelVisibilityMode="labeled"/>

</LinearLayout>