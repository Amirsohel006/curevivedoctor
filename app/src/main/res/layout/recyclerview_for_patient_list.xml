<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <TextView
        android:layout_marginTop="5dp"
        android:text="8:20PM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <FrameLayout
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:id="@+id/frameStackmarkspence"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/_260pxh">
        <LinearLayout
            android:id="@+id/linearColumnlanguage"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/group">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowmarkspence"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearContacts1Line"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.28"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStyleorange_300cornerRadius">
                    <ImageView android:id="@+id/imageImageOneOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="centerCrop"
                        android:src="@drawable/img_image1"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image1"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="start"
                    android:id="@+id/linearColumnmarkspence"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtMarkSpence"
                        style="@style/txtAssistantbold15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@string/lbl_mark_spence"
                        tools:text="@string/lbl_mark_spence" />

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGender"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/msg_male_25_diabe"
                        style="@style/txtAssistantregular10"
                        tools:text="@string/msg_male_25_diabe"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="Individual Reki"
                style="@style/txtAssistantbold16"
                tools:text="Individual Reki"/>
            <FrameLayout
                android:layout_gravity="end"
                android:id="@+id/frameStackduration"
                android:layout_height="@dimen/_30pxv"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_111pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtDuration"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_sunday_27_june"
                    style="@style/txtAssistantsemibold10"
                    tools:text="@string/msg_sunday_27_june"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtTime"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_08_00am_10_00"
                    style="@style/txtAssistantregular10"
                    tools:text="@string/msg_08_00am_10_00"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout
            android:id="@+id/frameStackellipsetwentyfive"
            android:layout_gravity="end"
            android:layout_height="@dimen/_93pxv"
            android:layout_marginBottom="@dimen/_31pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="wrap_content">
            <ImageView android:id="@+id/imageEllipseTwentyFive"
                android:layout_gravity="start"
                android:layout_height="@dimen/_78pxh"
                android:layout_width="@dimen/_81pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_ellipse25"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_ellipse25"/>
            <TextView android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/txtInProgress"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_29pxv"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_in_progress"
                style="@style/txtAssistantbold12"
                tools:text="@string/lbl_in_progress"/>
        </FrameLayout>
    </FrameLayout>



</LinearLayout>