<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="treatmentCardVM" type="com.curevivedoctor.app.modules.treatmentcard.data.viewmodel.TreatmentCardVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumntime"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_1pxh"
        android:layout_marginTop="@dimen/_1pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_blue_A700">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_234pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.56"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{treatmentCardVM.treatmentCardModel.txtTime}"
                    style="@style/txtRubikmedium14" tools:text="@string/lbl_09_20"/>
                <ImageView android:id="@+id/imageContrast"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_contrast"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_contrast"/>
                <ImageView android:id="@+id/imageUpload"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_17pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_upload"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_upload"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearTreatmentcard"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_4">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_bluegray_600"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_600"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTreatmentCard"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_180pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.89"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{treatmentCardVM.treatmentCardModel.txtTreatmentCard}"
                        style="@style/txtAssistantbold18" tools:text="@string/lbl_treatment_card"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitle"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_9pxh"
                    android:singleLine="true"
                    android:text="@{treatmentCardVM.treatmentCardModel.txtTitle}"
                    style="@style/txtPoppinssemibold16_4" tools:text="@string/lbl_case_history"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_101_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_write_here"
                    android:id="@+id/etGroup4481"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_30pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@={treatmentCardVM.treatmentCardModel.etGroup4481Value}"
                    android:textColorHint="@color/gray_800"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitleOne"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_9pxh"
                    android:singleLine="true"
                    android:text="@{treatmentCardVM.treatmentCardModel.txtTitleOne}"
                    style="@style/txtPoppinssemibold16_4" tools:text="@string/msg_treatment_refer"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnyoga"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtYoga"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtYoga}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_yoga"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/msg_back_bending_as"
                            android:id="@+id/etGroup4482"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etGroup4482Value}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumndiet"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDiet"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtDiet}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_diet"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/msg_low_calorie_die"
                            android:id="@+id/etGroup4483"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etGroup4483Value}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnherbology"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHerbology"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtHerbology}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_herbology"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/msg_custom_diabetes"
                            android:id="@+id/etGroupFour"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etGroupFourValue}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnaromatherapy"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAromaTherapy"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtAromaTherapy}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_aroma_therapy"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/msg_custom_diabetes2"
                            android:id="@+id/etGroupFive"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etGroupFiveValue}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnhydrotherapy"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHydrotherapy"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtHydrotherapy}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_hydrotherapy"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/msg_packs_applicati"
                            android:id="@+id/etLanguage"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etLanguageValue}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnaccupressure"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAccupressure"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{treatmentCardVM.treatmentCardModel.txtAccupressure}"
                            style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_accupressure"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_101_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/lbl_diabetic_points"
                            android:id="@+id/etGroupSeven"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@={treatmentCardVM.treatmentCardModel.etGroupSevenValue}"
                            android:textColorHint="@color/bluegray_801"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnsubmit"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSubmit"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_submit"
                        android:textAllCaps="false"
                        style="@style/btnGradientRounded" tools:text="@string/lbl_submit"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
