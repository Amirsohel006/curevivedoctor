<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerScreenTwoVM" type="com.curevivedoctor.app.modules.registerscreentwo.data.viewmodel.RegisterScreenTwoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameRegisterScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnellipse487"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <FrameLayout android:id="@+id/frameStackellipse487"
                    android:layout_height="@dimen/_52pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearRowellipse487"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_gradient_s_blue_a700_e_indigo_800_radius_26_49"
                            android:id="@+id/viewEllipse487"
                            android:layout_height="@dimen/_52pxh" android:layout_width="@dimen/_52pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_blue_a700_75_border_black_900_75_radius_26_49"
                            android:id="@+id/viewEllipse488"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_marginStart="@dimen/_61pxh" android:layout_width="@dimen/_52pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_26_49"
                            android:id="@+id/viewEllipse489"
                            android:layout_height="@dimen/_52pxh"
                            android:layout_marginStart="@dimen/_61pxh" android:layout_width="@dimen/_52pxh"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowcheckmark"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCheckmark"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLine316"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_weight="0.36" android:layout_width="0dp"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_weight="0.08"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtTwo}"
                            style="@style/txtPoppinssemibold24" tools:text="@string/lbl_2"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLine317"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_weight="0.36" android:layout_width="0dp"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.09"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtThree}"
                            style="@style/txtPoppinssemibold24_1" tools:text="@string/lbl_3"/>
                    </LinearLayout>
                </FrameLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameArrowbackios"
                    android:layout_height="@dimen/_37pxv"
                    android:layout_width="@dimen/_35pxh" style="@style/groupStylecornerRadius_4">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContactDetails"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_43pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtContactDetails}"
                    style="@style/txtPoppinssemibold20" tools:text="@string/lbl_contact_details"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnmobilenumber"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMobileNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtMobileNumber}"
                    style="@style/txtPoppinslight12" tools:text="@string/lbl_mobile_number"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_10"
                    android:ellipsize="end"
                    android:id="@+id/etRectangle6400"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@={registerScreenTwoVM.registerScreenTwoModel.etRectangle6400Value}"
                    style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnemailid"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmailID"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtEmailID}"
                    style="@style/txtPoppinslight12" tools:text="@string/lbl_email_id"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_10"
                    android:ellipsize="end"
                    android:id="@+id/etRectangle6400One"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@={registerScreenTwoVM.registerScreenTwoModel.etRectangle6400OneValue}"
                    style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnstate"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtState"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtState}"
                    style="@style/txtPoppinsmedium12_1" tools:text="@string/lbl_state"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_ex_bengaluru"
                    android:id="@+id/etGroup134"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={registerScreenTwoVM.registerScreenTwoModel.etGroup134Value}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumncity"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCity"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtCity}"
                    style="@style/txtPoppinsmedium12_1" tools:text="@string/lbl_city"/>
                <EditText
                    android:background="@drawable/rectangle_border_black_900_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_ex_bengaluru"
                    android:id="@+id/etGroup133"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={registerScreenTwoVM.registerScreenTwoModel.etGroup133Value}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnnext"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_114pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
