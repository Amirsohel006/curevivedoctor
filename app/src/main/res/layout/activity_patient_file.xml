<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="patientFileVM" type="com.curevivedoctor.app.modules.patientfile.data.viewmodel.PatientFileVM"/>
    </data>
    <FrameLayout android:id="@+id/framePatientFile"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_31pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="@dimen/_36pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_arrowleft_white_a700}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
            <LinearLayout android:id="@+id/linearRowfile"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackfile"
                    android:layout_height="@dimen/_35pxv" android:layout_width="@dimen/_134pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFile"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_47pxh"
                        android:layout_marginStart="@dimen/_47pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{patientFileVM.patientFileModel.txtFile}"
                        style="@style/txtAssistantbold24" tools:text="@string/lbl_file"/>
                    <View
                        android:background="@drawable/rectangle_bg_light_blue_a700_radius_2"
                        android:id="@+id/lineRectangle6376"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_134pxh"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDoctorsnotes"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.53"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{patientFileVM.patientFileModel.txtDoctorsnotes}"
                    style="@style/txtAssistantbold24_1" tools:text="@string/lbl_doctor_s_notes"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.02"
                    android:layout_width="0dp"
                    android:text="@{patientFileVM.patientFileModel.txtLanguage}"
                    style="@style/txtAssistantromanextrabold16" tools:text="@string/msg"/>
                <TextView android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.02"
                    android:layout_width="0dp"
                    android:text="@{patientFileVM.patientFileModel.txtLanguageOne}"
                    style="@style/txtAssistantromanextrabold16" tools:text="@string/msg"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumndescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.96"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:id="@+id/txtDescription"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:text="@{patientFileVM.patientFileModel.txtDescription}"
                        style="@style/txtAssistantbold16_1" tools:text="@string/msg_city_banglore"/>
                    <LinearLayout
                        android:id="@+id/linearRowrectangle6382"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_15"
                            android:id="@+id/viewRectangle6382"
                            android:layout_height="@dimen/_80pxh" android:layout_width="@dimen/_80pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_15"
                            android:id="@+id/viewRectangle6383"
                            android:layout_height="@dimen/_80pxh"
                            android:layout_marginStart="@dimen/_21pxh" android:layout_width="@dimen/_80pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_15"
                            android:id="@+id/viewRectangle6384"
                            android:layout_height="@dimen/_80pxh"
                            android:layout_marginStart="@dimen/_19pxh" android:layout_width="@dimen/_80pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
