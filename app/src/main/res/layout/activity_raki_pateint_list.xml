<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:id="@+id/linearColumnarrowLeftOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700_01">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            style="@style/groupStylewhite_A700_2">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_177pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleftOne"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_left"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrow_left"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSlots"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/msg_slots_availability"
                    style="@style/txtAssistantbold18_11"
                    tools:text="@string/msg_slots_availability"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>


        <ScrollView
            android:id="@+id/scrollview"
            android:fillViewport="true"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="0dp">
            <LinearLayout android:id="@+id/linearPatientslist"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@string/msg_choose_when_you_ll"
                    style="@style/txtPoppinsregular1211"
                    tools:text="@string/msg_choose_when_you_ll"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowmon"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearColumnmon"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_18pxv"
                        android:paddingEnd="@dimen/_6pxh"
                        android:paddingStart="@dimen/_6pxh"
                        android:paddingTop="@dimen/_18pxv"
                        style="@style/groupStylecornerRadius_41">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtMon"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_mon"
                            style="@style/txtPoppinsregular12_1"
                            tools:text="@string/lbl_mon"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumntue"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_7pxh"
                        android:paddingStart="@dimen/_7pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtTue"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_tue"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_tue"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnwed"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingStart="@dimen/_5pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtWed"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_wed"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_wed"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnthu"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_7pxh"
                        android:paddingStart="@dimen/_7pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtThu"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_thu"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_thu"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnfri"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtFri"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_fri"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_fri"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnsat"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtSat"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_sat"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_sat"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnsun"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_7pxh"
                        android:paddingStart="@dimen/_7pxh"
                        android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_51">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtSun"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_sun"
                            style="@style/txtPoppinsregular12_2"
                            tools:text="@string/lbl_sun"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSettimings"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_set_timings"
                    style="@style/txtPoppinsmedium201"
                    tools:text="@string/lbl_set_timings"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSlotCounter"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_slot_1"
                    style="@style/txtPoppinsregular14"
                    tools:text="@string/lbl_slot_1"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowupto"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_11pxv"
                    style="@style/groupStylecornerRadius_3">
                    <Spinner
                        android:background="@drawable/layer_list_bg_white_a700_01_border_black_900_5b_radius_6"
                        android:gravity="center_vertical"
                        android:id="@+id/spinnerTime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="120dp"
                        tools:listitem="@layout/spinner_item"/>
                    <LinearLayout android:id="@+id/linearColumnupto"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtUpto"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_upto"
                            style="@style/txtPoppinsregular10_1"
                            tools:text="@string/lbl_upto"/>
                        <ImageView android:id="@+id/imageArrow337One"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_337"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrow_337"/>
                    </LinearLayout>
                    <Spinner
                        android:background="@drawable/layer_list_bg_white_a700_01_border_black_900_5b_radius_6"
                        android:gravity="center_vertical"
                        android:id="@+id/spinnerTime1"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="120dp"
                        tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSlotCounter1"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_slot_2"
                    style="@style/txtPoppinsregular14"
                    tools:text="@string/lbl_slot_2"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowuptoOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_11pxv" style="@style/groupStylecornerRadius_3">
                    <Spinner
                        android:background="@drawable/layer_list_bg_white_a700_01_border_black_900_5b_radius_6"
                        android:gravity="center_vertical"
                        android:id="@+id/spinnerTime2"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="120dp" tools:listitem="@layout/spinner_item"/>
                    <LinearLayout android:id="@+id/linearColumnuptoOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtUptoOne"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_upto"
                            style="@style/txtPoppinsregular10_1"
                            tools:text="@string/lbl_upto"/>
                        <ImageView android:id="@+id/imageArrow337Three"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_337"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_337"/>
                    </LinearLayout>
                    <Spinner
                        android:background="@drawable/layer_list_bg_white_a700_01_border_black_900_5b_radius_6"
                        android:gravity="center_vertical"
                        android:id="@+id/spinnerTime3"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="120dp" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_28pxv" android:layout_width="@dimen/_112pxh">
                    <ImageView android:id="@+id/imageImageTwo"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_112pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageImageThree"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_add_circle"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_add_circle"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtButton"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_add_slots"
                            style="@style/txtPoppinssemibold141"
                            tools:text="@string/lbl_add_slots"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSet"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_set" android:textAllCaps="false"
            style="@style/btnGradientRounded" tools:text="@string/lbl_set"/>

    </LinearLayout>
</layout>
