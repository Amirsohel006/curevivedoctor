<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="slotsRowModel" type="com.curevivedoctor.app.modules.slots.data.model.SlotsRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowtime"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <TextView android:ellipsize="end" android:id="@+id/txtTime"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_11pxv"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_weight="0.69" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{slotsRowModel.txtTime}"
            style="@style/txtRobotoromanregular16" tools:text="@string/msg_7_00am_8_00_a"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumngroup310"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_140pxh"
            android:layout_weight="0.31" android:layout_width="0dp" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackgroup310"
                android:layout_height="@dimen/_26pxv" android:layout_width="@dimen/_56pxh">

                <com.google.android.material.switchmaterial.SwitchMaterial
                    android:id="@+id/switchMaterialGroup310"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh" android:layout_width="wrap_content"/>
                <View
                    android:background="@drawable/rectangle_bg_green_a700_0c_radius_13"
                    android:id="@+id/viewEllipseSix"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_26pxh"/>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtOff"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{slotsRowModel.txtOff}"
                style="@style/txtRobotoSerifrobotoserifregular12" tools:text="@string/lbl_off"/>
        </LinearLayout>
    </LinearLayout>
</layout>
