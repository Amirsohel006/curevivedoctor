<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listlanguageEightRowModel" type="com.curevivedoctor.app.modules.profileinfo.data.model.ListlanguageEightRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnlanguage"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius_2">
        <TextView android:ellipsize="end"
            android:id="@+id/txtLanguageEight"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/msg_qualification_p2"
            style="@style/txtPoppinsmedium16"
            tools:text="@string/msg_qualification_p2"/>
        <LinearLayout android:id="@+id/linearRowrectangle6403"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageRectangle6403"
                android:layout_height="@dimen/_80pxh"
                android:layout_width="@dimen/_82pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_rectangle6403_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6403_1"/>
            <ImageView android:id="@+id/imageRectangle6404"
                android:layout_height="@dimen/_80pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_width="@dimen/_82pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_rectangle6404_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6404_1"/>
        </LinearLayout>
    </LinearLayout>
</layout>
