<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listmarkspenceRowModel" type="com.curevivedoctor.app.modules.home.data.model.ListmarkspenceRowModel"/>
    </data>
    <FrameLayout
        android:id="@+id/frameStackmarkspence"
        android:layout_height="wrap_content"
        android:layout_width="@dimen/_250pxh">

        <LinearLayout
            android:id="@+id/linearColumnlanguage"
            style="@style/groupStylecornerRadius_15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginEnd="@dimen/_10pxh"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearRowmarkspence"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearContacts1Line"
                    style="@style/groupStyleorange_300cornerRadius"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.28"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageImageOneOne"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image1}"
                        android:layout_width="@dimen/_32pxh"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_gravity="start"
                        android:scaleType="centerCrop"
                        android:src="@drawable/img_image1"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image1" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearColumnmarkspence"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtMarkSpence"
                        style="@style/txtAssistantbold15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@string/lbl_mark_spence"
                        tools:text="@string/lbl_mark_spence" />

                    <TextView
                        android:id="@+id/txtGender"
                        style="@style/txtAssistantregular10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_male_25_diabe"
                        tools:text="@string/msg_male_25_diabe" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/txtLanguage"
                style="@style/txtAssistantbold16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{listmarkspenceRowModel.txtLanguage}"
                tools:text="@string/msg_fill_treatment" />

            <FrameLayout
                android:id="@+id/frameStackduration"
                android:layout_width="@dimen/_111pxh"
                android:layout_height="@dimen/_30pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_17pxv">

                <TextView
                    android:id="@+id/txtDuration"
                    style="@style/txtAssistantsemibold10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:ellipsize="end"
                    android:gravity="top|start"
                    android:singleLine="true"
                    android:text="@{listmarkspenceRowModel.txtDuration}"
                    tools:text="@string/msg_sunday_27_june" />

                <TextView
                    android:id="@+id/txtTime"
                    style="@style/txtAssistantregular10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:gravity="top|start"
                    android:singleLine="true"
                    android:text="@{listmarkspenceRowModel.txtTime}"
                    tools:text="@string/msg_08_00am_10_00" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameStackellipsetwentyfive"
            android:layout_gravity="end"
            android:layout_height="@dimen/_93pxv"
            android:layout_marginBottom="@dimen/_31pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="wrap_content">
            <ImageView android:id="@+id/imageEllipseTwentyFive"
                android:layout_gravity="start"
                android:layout_height="@dimen/_78pxh"
                android:layout_width="@dimen/_81pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_ellipse25}"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_ellipse25"/>
            <TextView android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/txtInProgress"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_29pxv"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_in_progress"
                style="@style/txtAssistantbold12"
                tools:text="@string/lbl_in_progress"/>
        </FrameLayout>
    </FrameLayout>
</layout>
