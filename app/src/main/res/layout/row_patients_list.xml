<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="patientsListRowModel" type="com.curevivedoctor.app.modules.patientslist.data.model.PatientsListRowModel"/>
    </data>
    <LinearLayout
        android:layout_marginTop="@dimen/_10pxh"
        android:id="@+id/linearRowellipse545"
        android:layout_height="100dp"
        android:layout_width="match_parent"
        android:orientation="horizontal"
       android:background="@drawable/backgrounfforpatient">
        <FrameLayout android:id="@+id/frameStackellipse545"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_64pxh">
            <ImageView android:id="@+id/imageEllipse545"
                android:layout_gravity="start"
                android:layout_height="@dimen/_64pxh"
                android:layout_width="@dimen/_64pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse545}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse545"/>
            <View
                android:background="@drawable/rectangle_bg_light_green_a700_border_white_a700_radius_5"
                android:id="@+id/viewEllipseFourteen"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_10pxh"/>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnpriya"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_weight="0.59" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtPriya"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{patientsListRowModel.txtPriya}"
                style="@style/txtAssistantbold20" tools:text="@string/lbl_priya"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGender"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{patientsListRowModel.txtGender}"
                style="@style/txtAssistantsemibold12_1" tools:text="@string/lbl_female_26_yrs"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtHypertension"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{patientsListRowModel.txtHypertension}"
                style="@style/txtAssistantsemibold12_1" tools:text="@string/lbl_hypertension"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackellipsefifteen"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="@dimen/_39pxh" style="@style/groupStyle">
            <View
                android:background="@drawable/rectangle_bg_light_blue_a701_radius_19_525"
                android:id="@+id/viewEllipseFifteen"
                android:layout_gravity="start"
                android:layout_height="@dimen/_36pxh" android:layout_width="@dimen/_39pxh"/>
            <ImageView android:id="@+id/imageUser"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_user"
                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
        </FrameLayout>
    </LinearLayout>
</layout>
